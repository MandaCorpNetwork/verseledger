{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "targetDefaults": {
    "test": {
      "outputs": ["{projectRoot}/TESTOUTPUT"],
      "cache": true
    },
    "lint": {
      "outputs": ["{projectRoot}/LINTOUTPUT"],
      "cache": true
    },
    "build": {
      "outputs": ["{projectRoot}/BUILDOUTPUT"],
      "cache": true
    }
  },
  "defaultBase": "main",
  "plugins": [
    {
      "plugin": "@nx/eslint/plugin",
      "options": {
        "targetName": "lint"
      }
    },
    {
      "plugin": "@nx/vite/plugin",
      "options": {
        "buildTargetName": "build",
        "testTargetName": "test",
        "serveTargetName": "serve",
        "previewTargetName": "preview",
        "serveStaticTargetName": "serve-static",
        "typecheckTargetName": "typecheck"
      }
    }
  ],
  "nxCloudId": "6783331baf0e0efc66ea3181",
  "namedInputs": {
    "sharedGlobals": ["{workspaceRoot}/.github/workflows/nx-ci.yml"],
    "default": ["sharedGlobals"]
  }
}
