{
  "name": "verseledger",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "services/*",
    "apps/*"
  ],
  "scripts": {
    "postinstall": "preconstruct dev && manypkg check",
    "clean": "manypkg exec rm -rf node_modules && manypkg exec rm -rf dist && rm -rf node_modules"
  },
  "dependencies": {
    "@changesets/cli": "^2.27.5",
    "@manypkg/cli": "^0.21.4",
    "@preconstruct/cli": "^2.8.4",
    "concurrently": "8.2.2"
  },
  "preconstruct": {
    "packages": [
      "services/*",
      "apps/*"
    ]
  }
}
